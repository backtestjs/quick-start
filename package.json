{
  "name": "quick-start",
  "version": "1.1.0",
  "description": "This project is a quick start guide for Backtest, a library for trading developers",
  "main": "dist/main.js",
  "keywords": [
    "backtesting",
    "backtest",
    "boilerplate",
    "quick-start",
    "project-template"
  ],
  "scripts": {
    "align-db": "npx prisma validate && npx prisma generate && npx prisma db push",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "cd dist && node main.js",
    "dev": "node -r ts-node/register --env-file=.env main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "prisma": {
    "schema": "./node_modules/@backtest/framework/prisma/schema.prisma"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@backtest/framework": "^1.1.4",
    "dayjs": "^1.11.13",
    "technicalindicators": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "prisma": "^5.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
